    ],
    "final": "Proxy",
    "auto_detect_interface": true,
    "default_domain_resolver": { "server": "alidns" }
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_fakeip": false
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9090",
