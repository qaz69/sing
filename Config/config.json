{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "providers": [
    {
      "tag": "One",
      "type": "remote",
      "path": "./providers/One.yaml",
      "url": "youurl",
      "health_check": {
        "enabled": true,
        "url": "https://www.gstatic.com/generate_204",
        "interval": "10m",
        "timeout": "30s"
      }
    }
  ],
  "dns": {
    "servers": [
      { "tag": "alidns", "type": "udp", "server": "223.5.5.5" },
      { "tag": "local", "type": "udp", "server": "116.116.116.116" },
      { "tag": "cloudflare", "type": "https", "server": "1.1.1.1", "detour": "Proxy" },
      { "tag": "fakeip", "type": "fakeip", "inet4_range": "198.18.0.0/15", "inet6_range": "fc00::/18" }
    ],
    "rules": [
      { "clash_mode": "direct", "server": "local" },
      { "clash_mode": "global", "server": "cloudflare" },
      { "query_type": "HTTPS", "action": "reject" },
      { "domain_suffix": ["services.googleapis.cn", "googleapis.cn", "xn--ngstr-lra8j.com"], "server": "cloudflare" },
      { "rule_set": ["geosite-private", "geosite-cn", "geosite-steamcn", "geosite-apple"], "server": "local" },
      { "rule_set": ["geosite-category-ads"], "server": "reject" },
      { "query_type": ["A", "AAAA"], "server": "fakeip", "rewrite_ttl": 1 }
    ],
    "final": "cloudflare",
    "strategy": "ipv4_only",
    "independent_cache": true,
    "reverse_mapping": true
  },
  "inbounds": [
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "::",
      "listen_port": 7891,
      "sniff": true,
      "sniff_override_destination": true
    },
    {
      "type": "direct",
      "tag": "dns-in",
      "listen": "::",
      "listen_port": 1053
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "::",
      "listen_port": 7890,
      "sniff": true,
      "sniff_override_destination": true
    }
  ],
  "outbounds": [
    { "tag": "阻止", "type": "block" },
    { "tag": "直连", "type": "direct" },
    { "tag": "Youtube", "type": "selector", "providers": ["One"] },
    { "tag": "Proxy", "type": "selector", "providers": ["One"] },
    { "tag": "Google", "type": "selector", "providers": ["One"] },
    { "tag": "Telegra", "type": "selector", "providers": ["One"] },
    { "tag": "Emby", "type": "selector", "providers": ["One"] },
    { "tag": "OpenAI", "type": "selector", "providers": ["One"] },
    { "tag": "GLOBAL", "type": "selector", "outbounds": ["Proxy", "OpenAI", "Youtube", "Google", "Telegra", "直连"] }
  ],
  "route": {
    "rules": [
      { "action": "sniff", "sniffer": ["http", "tls", "quic", "dns"] },
      { "inbound": "dns-in", "action": "hijack-dns" },
      { "clash_mode": "global", "outbound": "GLOBAL" },
      { "ip_is_private": true, "outbound": "直连" },
      { "clash_mode": "direct", "outbound": "直连" },
      { "rule_set": "geoip-apple", "outbound": "直连" },
      { "rule_set": "geosite-apple", "outbound": "直连" },
      { "rule_set": "geosite-steamcn", "outbound": "直连" },
      { "rule_set": "geosite-ai", "outbound": "OpenAI" },
      { "rule_set": "geoip-telegram", "outbound": "Telegra" },
      { "rule_set": "geosite-youtube", "outbound": "Youtube" },
      { "rule_set": "geosite-spotify", "outbound": "Proxy" },
      { "rule_set": "geosite-telegram", "outbound": "Telegra" },
      { "rule_set": "geosite-onedrive", "outbound": "Proxy" },
      { "rule_set": "geosite-microsoft", "outbound": "Proxy" },
      { "domain_suffix": ["sgameglobal.com"], "outbound": "Proxy" },
      { "domain_suffix": ["jsq.vban.xyz", "us.nanflix.net", "cn2gias.uk", "mjji.de"], "outbound": "Emby" },
      { "rule_set": "geosite-category-ads", "outbound": "阻止" },
      { "rule_set": "geoip-google", "outbound": "Google" },
      { "rule_set": "geosite-google", "outbound": "Google" },
      { "rule_set": "geosite-googlecn", "outbound": "Google" },
      { "rule_set": "geosite-cn", "outbound": "直连" },
      { "rule_set": "geoip-cn", "outbound": "直连" }
    ],
    "rule_set": [
      { "tag": "geosite-ai", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/category-ai-!cn.srs", "download_detour": "Proxy" },
      { "tag": "geosite-youtube", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/youtube.srs", "download_detour": "Proxy" },
      { "tag": "geosite-googlecn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google@cn.srs", "download_detour": "Proxy" },
      { "tag": "geosite-google", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/google.srs", "download_detour": "Proxy" },
      { "tag": "geosite-onedrive", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/onedrive.srs", "download_detour": "Proxy" },
      { "tag": "geosite-microsoft", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/microsoft.srs", "download_detour": "Proxy" },
      { "tag": "geosite-apple", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/apple.srs", "download_detour": "Proxy" },
      { "tag": "geosite-spotify", "type": "remote", "format": "binary", "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/spotify.srs", "download_detour": "Proxy" },
      { "tag": "geosite-telegram", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/telegram.srs", "download_detour": "Proxy" },
      { "tag": "geosite-steamcn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/steam@cn.srs", "download_detour": "Proxy" },
      { "tag": "geosite-private", "type": "remote", "format": "binary", "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/private.srs", "download_detour": "Proxy" },
      { "tag": "geosite-cn", "type": "remote", "format": "binary", "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geosite/cn.srs", "download_detour": "Proxy" },
      { "tag": "geosite-category-ads", "type": "remote", "format": "binary", "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/refs/heads/sing/geo/geosite/category-ads.srs", "download_detour": "Proxy" },
